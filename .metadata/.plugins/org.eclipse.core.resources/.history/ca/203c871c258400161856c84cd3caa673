package _09_loops;
import java.util.*;

public class _02_for 
{
	public static void main(String[] args) 
	{
		Scanner input= new Scanner(System.in);
		
		System.out.println("Input your text here:");
	    String userCount = input.next();

		int count =0;
		for (int i=0; i<userCount.length(); ++i)
		{
			switch (userCount.charAt(i))
			{
	        	case 'a':
	        	case 'e':
	        	case 'i':
	        	case 'o':
	        	case 'u':				
				count++;
				break;
				default:
			}
		}
		System.out.println(count);		    
		input.close();
	}

}
